pipeline {
    agent none 
    stages {
        stage('non-parallel stage '){
            agent {
                label 'master'
            }
            steps{
                echo "this step will run first from agent master !"
            }

        }
        stage ('run Tests'){
            parallel{
                stage('Run on Windows'){
                    agent {
                        label 'Windows_Node'
                    }
                    steps{
                        echo "test on windows !"
                    }
                }
                stage('Run on Master'){
                    agent {
                        label 'master'
                    }
                    steps{
                        echo "test on master !"
                    }
                }
            }
        }
    }
}